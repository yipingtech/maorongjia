          	<interceptor-ref name="token">
            	<!-- includeMethods表示包含指定的方法， 即对标记为includeMethods的方法进行拦截 -->
                <param name="includeMethods">add,update</param>
            	<!-- 定义被排除的方法名， 也就是你action中不被这个拦截器拦截的方法名 -->
