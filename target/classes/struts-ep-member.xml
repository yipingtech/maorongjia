		<!-- 积分规则 -->
		<action name="integralRuleAction" class="integralRuleAction">
			<result>/WEB-INF/jsp/enterprise/finance/integralRule.jsp</result>
			<result name="viewPage">/WEB-INF/jsp/enterprise/finance/integralRule_view.jsp</result>
